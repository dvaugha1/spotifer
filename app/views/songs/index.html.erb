<div class="header">
  <h2> Spotifer </h2>
  <img src="https://encrypted-tbn2.gstatic.com/images?q=tbn:ANd9GcSBjCrgjQNC7aTn7FV4mxXVmxBITCv5RTp7JMI8a1RNFdGM4WEnLA" id="logo">
    <h1> Search Page </h1>
      <div class='top-buttons'>
        <%= link_to 'Go to Your Profile Page', users_show_path(current_user), class: "btn btn-default btn-sm" %>
      </div>
</div>


  <div class="form-group form-inline searchWrap">  <div>
          <%= form_tag '/search', method: 'get' do %>
          <%= text_field_tag 'title',
                        params[:title],
                        :class => "form-control input-lg searchBox",
                        :placeholder => 'Search by Artist' %>
  </div>

  <div>
            <%= button_tag(type: "submit", class: "btn btn-primary btn-lg searchButton") do %>
              Search <i class="fa fa-sign-out"></i>
            <% end %>
          <% end %>
      </div>
  </div>



